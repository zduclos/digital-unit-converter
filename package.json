{
  "name": "digital-unit-converter",
  "version": "1.0.0",
  "description": "Utility to simplify conversion of digital units of measurement",
  "author": "Zach Duclos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:zduclos/digital-unit-converter.git"
  },
  "bugs": {
    "url": "https://github.com/zduclos/digital-unit-converter/issues"
  },
  "homepage": "https://github.com/zduclos/digital-unit-converter#readme",
  "keywords": [
    "convert",
    "unit",
    "decimal",
    "binary",
    "bit",
    "byte",
    "gibibyte",
    "gigabyte",
    "kibibyte",
    "kilobyte",
    "mebibyte",
    "megabyte"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
